<p><%= l(:label_version_updated) %></p>
<ul>
<li><strong><%= l(:label_project) %></strong>: <%=h @project.name %></li>
<li><strong><%= l(:label_version) %></strong>:
<%= @version.name_was + " -> " if @version.name_changed? %>
<%= link_to @version.name, @version_url %><br />
<%=h @version.description %></li>
<li><strong><%= l(:field_effective_date) %></strong>:
<%= @effective_date_old + " -> " if @version.effective_date_changed? %>
<%= @effective_date_new %></li>
<li><strong><%= l(:field_status) %></strong>:
<%= l("version_status_#{@version.status_was}") + " -> " if @version.status_changed? %>
<%= l("version_status_#{@version.status}") %></li>
<li><strong><%= l(:label_wiki_page) %></strong>: <%= link_to @version.wiki_page_title, @wiki_url unless @version.wiki_page_title.blank? %></li>
</ul>

